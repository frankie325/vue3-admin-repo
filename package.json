{
  "name": "vue-admin",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx,d.ts,vue,tsx}\" --fix --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint \"**/*.{vue,less,postcss,css,html}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,json,ts,tsx,d.ts,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.0",
    "crypto-js": "^4.1.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
